{% if menu is defined and menu %}
    <ul class="menu">
        {% for item in menu %}
            <li class="{{ item.classes | join(' ') }} menu__item">
                <a target="{{ item.target }}" href="{{ item.link }}" class="menu__link">{{ item.title }}</a>
                {% include "partial/menu.twig" with {'menu': item.get_children} %}
            </li>
        {% endfor %}
    </ul>
{% endif %}
